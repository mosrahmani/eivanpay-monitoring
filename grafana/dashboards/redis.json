{
  "id": null,
  "uid": "redis-metrics",
  "title": "Redis Cache Metrics",
  "tags": ["redis", "cache", "eivan-pay"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 0,
  "refresh": "30s",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Redis Exporter Status",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 0, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "up{job=\"eivan-pay-redis\"}",
          "legendFormat": "Status",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "mappings": [
            {"type": "value", "value": "0", "text": "Down"},
            {"type": "value", "value": "1", "text": "Up"}
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Redis Uptime",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 4, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "redis_uptime_in_seconds",
          "legendFormat": "Uptime",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {"mode": "thresholds"}
        }
      }
    },
    {
      "id": 3,
      "title": "Connected Clients",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 8, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "redis_connected_clients",
          "legendFormat": "Clients",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 50, "color": "yellow"},
              {"value": 100, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Memory Usage",
      "type": "gauge",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
          "legendFormat": "Memory %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 70, "color": "yellow"},
              {"value": 90, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Hit Rate",
      "type": "gauge",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
          "legendFormat": "Hit Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 50, "color": "yellow"},
              {"value": 80, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Commands Processed Per Second",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(redis_commands_processed_total[5m])",
          "legendFormat": "Commands/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops"
        }
      }
    },
    {
      "id": 7,
      "title": "Memory Usage Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "legendFormat": "Used",
          "refId": "A"
        },
        {
          "expr": "redis_memory_max_bytes",
          "legendFormat": "Max",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "bytes"
        }
      }
    },
    {
      "id": 8,
      "title": "Keyspace Hits vs Misses",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(redis_keyspace_hits_total[5m])",
          "legendFormat": "Hits",
          "refId": "A"
        },
        {
          "expr": "rate(redis_keyspace_misses_total[5m])",
          "legendFormat": "Misses",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops"
        }
      }
    },
    {
      "id": 9,
      "title": "Connected Clients Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "redis_connected_clients",
          "legendFormat": "Connected Clients",
          "refId": "A"
        },
        {
          "expr": "redis_blocked_clients",
          "legendFormat": "Blocked Clients",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short"
        }
      }
    },
    {
      "id": 10,
      "title": "Keys by Database",
      "type": "stat",
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 20},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "redis_db_keys",
          "legendFormat": "DB{{db}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "short"
        }
      }
    },
    {
      "id": 11,
      "title": "Evicted Keys",
      "type": "stat",
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 20},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "redis_evicted_keys_total",
          "legendFormat": "Evicted",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 1000, "color": "yellow"},
              {"value": 10000, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 12,
      "title": "Expired Keys",
      "type": "stat",
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 20},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "redis_expired_keys_total",
          "legendFormat": "Expired",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "short"
        }
      }
    },
    {
      "id": 13,
      "title": "Network I/O",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(redis_net_input_bytes_total[5m])",
          "legendFormat": "Input",
          "refId": "A"
        },
        {
          "expr": "rate(redis_net_output_bytes_total[5m])",
          "legendFormat": "Output",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "Bps"
        }
      }
    },
    {
      "id": 14,
      "title": "Commands by Type",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "topk(10, rate(redis_commands_total[5m]))",
          "legendFormat": "{{cmd}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops"
        }
      }
    }
  ]
}
